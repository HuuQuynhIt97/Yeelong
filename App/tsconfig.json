{
  "compileOnSave": false,
  "compilerOptions": {
    "paths": {
      "account": [
        "dist/account/account",
        "dist/account"
      ],
      "@common/*": [
        "app/common/*"
      ],
      "@common": [
        "app/common/index.ts"
      ]
    },
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
